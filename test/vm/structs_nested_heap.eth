// EXPECTED_OUTPUT: Outer: 1, 10, 20, 2
// EXPECTED_OUTPUT: Updated: 1, 11, 40, 2

#include "std/io.eth"

struct Inner {
    i32 a;
    i32 b;
}

struct Outer {
    i32 x;
    Inner in;
    i32 y;
}

void main() {
    [1]Outer o;
    o[0].x = 1;
    o[0].in.a = 10;
    o[0].in.b = 20;
    o[0].y = 2;
    
    printf("Outer: %d, %d, %d, %d\n", o[0].x, o[0].in.a, o[0].in.b, o[0].y);
    
    o[0].in.a++;
    o[0].in.b = o[0].in.b * 2;
    
    printf("Updated: %d, %d, %d, %d\n", o[0].x, o[0].in.a, o[0].in.b, o[0].y);
}
