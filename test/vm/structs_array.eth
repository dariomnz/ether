// EXPECTED_OUTPUT: Heap Point: 42, 84
// EXPECTED_OUTPUT: Updated Heap Point: 41, 42
#include "std/io.eth"

struct Point {
    i32 x;
    i32 y;
}

void main() {
    [1]Point p;
    
    p[0].x = 42;
    p[0].y = 84;
    
    printf("Heap Point: %d, %d\n", p[0].x, p[0].y);
    
    p[0].x--;
    p[0].y = p[0].y / 2;
    
    printf("Updated Heap Point: %d, %d\n", p[0].x, p[0].y);
}
