#include "std/io.eth"

i32 main() {
    string s1 = "Hello";
    string s2 = "World";
    string s3 = s1 + s2;
    string s4 = s1 + " " + s2;

    printf("%s\n", s3);

    s4[0] = ' ';
    printf("%s\n", s4);

    printf("Test String Length: %d\n", strlen("Hello World!"));

